{
    "name": "igorbalden/sf5-api-slim3-client",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "monolog/monolog": "^1.17",
        "slim/slim": "^3.1",
        "slim/twig-view": "2.*",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8010 -t public",
        "test": "phpunit"
    }
}
